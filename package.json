{
  "name": "natours",
  "version": "1.0.0",
  "description": "learning Node.js Express.js and MongoDB with mongoose ORM",
  "license": "ISC",
  "author": "Gokulrajan Natarajan",
  "type": "commonjs",
  "alias": {
    "buffer": false,
    "process": false,
    "stream": false
  },
  "targets": {
    "main": false,
    "default": {
      "source": "public/js/index.js"
    }
  },
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js",
